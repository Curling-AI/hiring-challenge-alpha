{
  "name": "multi-source-ai-agent",
  "version": "1.0.0",
  "description": "AI Agent that answers questions using multiple data sources",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && concurrently \"npm run server\" \"npm run client\"",
    "server": "SQLITE_DB_PATH='./data/sqlite/music.db' node dist/server.js",
    "client": "vite",
    "dev:server": "tsx watch src/server.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaqueu-1/hiring-challenge-alpha.git"
  },
  "keywords": [
    "ai",
    "agent",
    "langchain",
    "sqlite",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zaqueu-1/hiring-challenge-alpha/issues"
  },
  "homepage": "https://github.com/zaqueu-1/hiring-challenge-alpha#readme",
  "dependencies": {
    "@langchain/core": "^0.3.49",
    "@langchain/google-genai": "^0.2.5",
    "@langchain/langgraph": "^0.0.10",
    "@langchain/openai": "^0.5.7",
    "@types/node": "^20.11.24",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "langchain": "^0.3.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "vite": "^5.1.3"
  }
}
